  "main": "index.js",
  "scripts": {
    "build": "npm run build-web && npm run build-server",
    "build-web": "cd ./web npm install --only=dev && npm install && npm run build",
    "build-server": "cd ./server npm install && npm run postinstall && npm run prisma && npm start",
    "prisma": "npx prisma migrate deploy",
    "postinstall": "cd ./server tsc",
    "start": "cd ./server/dist node index.js"
  },
